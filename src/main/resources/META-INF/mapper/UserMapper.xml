<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dzq.group.mark.mapper.UserMapper">
    <insert id="insert" parameterType="dzq.group.mark.entity.GmUser">
        INSERT INTO USER (USER_ID,MOBILE_PHONE,PASSWORD,NICK_NAME,USER_TYPE,CRE_DATE,MOD_DATE)
        VALUES(#{userId,jdbcType=VARCHAR},#{mobilePhone,jdbcType=VARCHAR},#{password,jdbcType=VARCHAR}
        ,#{nickName,jdbcType=VARCHAR},#{userType,jdbcType=VARCHAR},now(),now()
        )
    </insert>

</mapper>  